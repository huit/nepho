#!/usr/bin/env python
from cement.core import handler, hook
from nepho import cli
import sys


def main():
    # Load the base Nepho cement controller
    app = cli.base.Nepho()

    try:
        app.setup()
        app.run()
    finally:
        app.close()

if __name__ == '__main__':
    sys.exit(main())

